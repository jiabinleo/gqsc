.list-wrap {
  padding: 0 35rpx;
  font-size: 22rpx;
}
.list-wrap .list {
  border-bottom: solid 1rpx #d3d3d3;
  padding: 35rpx 0;
  overflow: hidden;
  height: 150rpx;
}
.list-wrap .list .left {
  float: left;
  width: 220rpx;
}
.list-wrap .list .left image {
  width: 220rpx;
  height: 146rpx;
  border-radius: 10rpx;
}
.list-wrap .list .center {
  width: 356rpx;
  margin-left: 16rpx;
  float: left;
  margin-right: 12rpx;
}
.list-wrap .list .center .title {
  text-overflow: -o-ellipsis-lastline;
  overflow: hidden;
  text-overflow: ellipsis;
  display: -webkit-box;
  -webkit-line-clamp: 1;
  -webkit-box-orient: vertical;
  align-content: center;
  height: 40rpx;
  color: #333333;
  line-height: 40rpx;
  font-size: 30rpx;
}
.list-wrap .list .center .detail {
  font-size: 28rpx;
  text-overflow: -o-ellipsis-lastline;
  overflow: hidden;
  text-overflow: ellipsis;
  display: -webkit-box;
  -webkit-line-clamp: 2;
  -webkit-box-orient: vertical;
  align-content: center;
  height: 80rpx;
  color: #666666;
}
.list-wrap .list .center .areaTime {
  font-size: 20rpx;
}
.list-wrap .list .center .areaTime text {
  color: #999999;
}
.list-wrap .list .center .areaTime text:last-of-type {
  padding-left: 10rpx;
}
.list-wrap .list .right {
  position: relative;
  width: 60rpx;
  float: right;
  height: 150rpx;
}
.list-wrap .list .right image {
  position: absolute;
  top: 50%;
  transform: translateY(-50%);
  width: 50rpx;
  height: 50rpx;
}
